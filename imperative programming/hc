sum_numbers = 0
print("please enter your numbers.")
number = int(input())
while number !=0:
    sum_numbers += number #telt vanaf 0
    number = int(input()) #stopt totdat alles van de input is opgeteld bij elkaar
print("the total sum of those numbers equals", sum_numbers)
